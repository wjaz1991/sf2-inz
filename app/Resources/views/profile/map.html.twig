{% extends 'base.html.twig' %}

{% block body %}
    <div class="col-md-12 text-center">
    {% if map %}
        {{ google_map(map) }}
    {% else %}
        <div class="alert alert-warning">
            Failed to load a map. Probably you haven't specified your address.
        </div>
    {% endif %}
    </div>
{% endblock %}